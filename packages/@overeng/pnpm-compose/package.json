{
  "name": "@overeng/pnpm-compose",
  "version": "0.1.0",
  "private": true,
  "description": "CLI for composing pnpm workspaces with git submodules",
  "type": "module",
  "exports": {
    ".": "./src/mod.ts",
    "./cli": "./src/cli.ts"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/mod.js",
      "./cli": "./dist/cli.js"
    }
  },
  "dependencies": {
    "@overeng/utils": "file:../utils"
  },
  "devDependencies": {
    "@effect/cli": "0.73.0",
    "@effect/platform": "0.94.1",
    "@effect/platform-node": "0.104.0",
    "@effect/vitest": "0.27.0",
    "@types/node": "25.0.3",
    "effect": "3.19.14",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  },
  "peerDependencies": {
    "@effect/cli": "^0.73.0",
    "@effect/opentelemetry": "^0.60.0",
    "@effect/platform": "^0.94.1",
    "@effect/platform-node": "^0.104.0",
    "@effect/printer": "^0.47.0",
    "@effect/printer-ansi": "^0.47.0",
    "@effect/rpc": "^0.73.0",
    "@playwright/test": "^1.57.0",
    "effect": "^3.19.14"
  },
  "$genie": {
    "source": "package.json.genie.ts",
    "warning": "DO NOT EDIT - changes will be overwritten"
  },
  "patchedDependencies": {
    "effect-distributed-lock@0.0.11": "../utils/patches/effect-distributed-lock@0.0.11.patch"
  }
}
