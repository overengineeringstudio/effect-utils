// Bun Snapshot v1, https://bun.sh/docs/test/snapshots

exports[`status command output renders workspace with problems (CRITICAL + WARNING sections) 1`] = `
"workspace: <WORKSPACE>

 CRITICAL 

  missing-dep missing
    fix: dotdot clone missing-dep
    fix: git clone <url> missing-dep
    skip: dotdot ignore missing-dep

 WARNING 

  project-b diverged (local: <SHORT_HASH>, remote: <SHORT_HASH>)
    fix: cd project-b && git pull --rebase
    fix: dotdot sync project-b
    skip: dotdot ignore project-b --diverged

  project-a diverged (local: <SHORT_HASH>, remote: <SHORT_HASH>)
    fix: cd project-a && git pull --rebase
    fix: dotdot sync project-a
    skip: dotdot ignore project-a --diverged

  3 repos have uncommitted changes
    project-c, project-b, project-a
    fix: dotdot commit -a
    fix: git status <repo> to review

────────────────────────────────────────

project-c main@<SHORT_HASH> * ← shared-lib

project-b main@<SHORT_HASH> * ↕<SHORT_HASH> ← shared-lib
  packages(12):
    @scope/package-one
    @scope/package-two
    @scope/package-three
    @scope/package-four
    @scope/package-five
    + 7 more

project-a main@<SHORT_HASH> * ↕<SHORT_HASH> ← shared-lib

3 members · 1 deps"
`;

exports[`status command output renders clean workspace (no problems) 1`] = `
"workspace: <WORKSPACE>

 WARNING 

  2 repos have uncommitted changes
    repo-b, repo-a
    fix: dotdot commit -a
    fix: git status <repo> to review

────────────────────────────────────────

repo-b main@<SHORT_HASH> *

repo-a main@<SHORT_HASH> *

2 members · 0 deps"
`;
