{
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
    "lib": ["ES2023", "DOM"],
    "rootDir": "src",
    "outDir": "./dist",
    "jsx": "react-jsx",
    "allowJs": true,
    "checkJs": false,
    "strict": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "exactOptionalPropertyTypes": false,
    "noUncheckedIndexedAccess": false,
    "verbatimModuleSyntax": false,
    "noImplicitReturns": false
  },
  "include": ["src/**/*"]
}
